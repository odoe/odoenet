{
	"name": "odoenet",
	"version": "1.0.0",
	"scripts": {
		"dev": "dojo build",
		"deploy": "aws s3 rm s3://odoe.net --recursive && aws s3 sync output/dist s3://learn-dojo.com",
		"build": "dojo build --mode dist --dojorc prod.dojorc",
		"build:dev": "dojo build -m=dev -s -w",
		"test": "dojo test",
		"test:unit": "dojo build --mode unit && dojo test --unit --config local",
		"test:functional": "dojo build --mode functional && dojo test --functional --config local",
		"test:all": "dojo build --mode unit && dojo build --mode functional && dojo test --all --config local"
	},
	"dependencies": {
		"@dojo/framework": "7.0.5",
		"@dojo/widgets": "7.0.5",
		"regenerator-runtime": "0.13.7",
		"tslib": "1.10.0"
	},
	"devDependencies": {
		"@dojo/cli": "7.0.0",
		"@dojo/cli-build-app": "8.0.0-alpha.3",
		"@dojo/cli-test-intern": "7.0.0",
		"@mapbox/rehype-prism": "0.5.0",
		"@types/fs-extra": "9.0.5",
		"@types/imagemin": "7.0.0",
		"@types/imagemin-webp": "5.1.1",
		"@types/node": "14.14.14",
		"canonical-path": "1.0.0",
		"feed": "4.2.1",
		"fs-extra": "8.1.0",
		"hast-to-hyperscript": "9.0.1",
		"imagemin": "7.0.1",
		"imagemin-webp": "6.0.0",
		"rehype-picture": "3.0.0",
		"remark-external-links": "8.0.0",
		"remark-frontmatter": "3.0.0",
		"remark-iframes": "4.0.4",
		"remark-parse": "9.0.0",
		"remark-parse-yaml": "0.0.3",
		"remark-rehype": "8.0.0",
		"remark-slug": "6.0.0",
		"typescript": "3.7.3",
		"unified": "9.2.0"
	},
	"prettier": {
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true,
		"printWidth": 120,
		"arrowParens": "always"
	}
}
